					vc-id = <1>;
					port-index = <2>;
					bus-width = <4>;
					remote-endpoint = <&jetson_csi_out3>;
				};
			};
			port@4 {
				reg = <4>;
				jetson_vi_in4: endpoint {
					vc-id = <2>;
					port-index = <2>;
					bus-width = <4>;
