				lane_polarity = "0";
				dynamic_pixel_bit_depth = "16";
				csi_pixel_bit_depth = "16";
				mode_type = "yuv";
				pixel_phase = "uyvy";

				active_w = "1280";
				active_h = "720";
				readout_orientation = "0";
				line_length = "1280";
				inherent_gain = "1";
				pix_clk_hz = "240000000";
				serdes_pix_clk_hz = "240000000";

				gain_factor = "100";
				min_gain_val = "100"; /* dB */
				max_gain_val = "1600"; /* dB */
				step_gain_val = "1"; /* 0.1 */
				default_gain = "100";
				min_hdr_ratio = "1";
				max_hdr_ratio = "1";
				framerate_factor = "1000000";
				min_framerate = "30000000";
				max_framerate = "30000000";
				step_framerate = "1";
				default_framerate = "30000000";
				exposure_factor = "1000000";
